        <property name="styleSheet">
         <string notr="true">QLineEdit {
    /* 基础样式 */
    background-color: #f0f8ff;  /* 浅蓝色背景 */
    border: 1px solid #b0d0e8;  /* 浅蓝色边框 */
    border-radius: 6px;         /* 圆角 */
    color: #2c3e50;             /* 深蓝色文字 */
    font-size: 13px;
    font-family: &quot;Microsoft YaHei&quot;, sans-serif;
    
    /* 选中文本样式 */
    selection-background-color: #4da6ff;  /* 选中文本背景色 */
    selection-color: white;               /* 选中文本颜色 */
}

/* 鼠标悬停状态 */
QLineEdit:hover {
    border: 1px solid #80c0ff;  /* 悬停时边框变亮 */
    background-color: #e6f2ff;  /* 悬停时背景稍微加深 */
}

/* 获得焦点状态 */
QLineEdit:focus {
    border: 1px solid #4da6ff;  /* 焦点状态使用更亮的蓝色 */
    background-color: #f7fbff;  /* 焦点状态背景色 */
}

/* 禁用状态 */
QLineEdit:disabled {
    background-color: #e6e6e6;  /* 灰色背景 */
    color: #999999;            /* 灰色文字 */
    border: 1px solid #cccccc; /* 灰色边框 */
}

/* 只读状态 */
QLineEdit:read-only {
    background-color: #f5f5f5;  /* 浅灰色背景 */
    color: #666666;            /* 深灰色文字 */
}</string>
